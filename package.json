{
  "name": "marriage",
  "version": "1.0.0",
  "keywords": [
    "expo",
    "boilerplate",
    "firebase",
    "native-base",
    "typescript"
  ],
  "author": "Jonathan Gan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/duapp/expo-fire-native/issues"
  },
  "homepage": "https://github.com/duapp/expo-fire-native#readme",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint",
    "test": "jest",
    "test:dev": "jest -o --watch",
    "prepare": "husky install",
    "eject": "expo eject"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@walletconnect/react-native-dapp": "^1.7.8",
    "Buffer": "^0.0.0",
    "base-64": "^1.0.0",
    "clarifai": "^2.9.1",
    "expo": "^45.0.0",
    "expo-asset": "~8.5.0",
    "expo-auth-session": "~3.6.0",
    "expo-camera": "~12.2.0",
    "expo-constants": "~13.1.1",
    "expo-crypto-polyfills": "^1.1.0",
    "expo-font": "~10.1.0",
    "expo-image-picker": "~13.1.1",
    "expo-linking": "~3.1.0",
    "expo-localization": "~13.0.0",
    "expo-random": "~12.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-updates": "~0.13.1",
    "expo-web-browser": "~10.2.0",
    "firebase": "8.2.3",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "native-base": "^3.2.1",
    "postcss": "^8.4.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "^18.1.0",
    "react-native": "0.68.1",
    "react-native-dotenv": "^3.2.0",
    "react-native-gesture-handler": "~2.2.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-web": "0.17.7",
    "react-redux": "^8.0.1",
    "styled-components": "^5.3.1",
    "styled-system": "^5.1.5",
    "twrnc": "^3.3.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^8.0.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.6.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": ">=6",
    "jest-expo": "^45.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.3.5"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/web-build/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 50,
        "branches": 50,
        "functions": 50,
        "lines": 50
      }
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "jest --coverage=false --bail --findRelatedTests"
    ]
  },
  "private": true
}
